<odoo>
    <data>
<record id="caretaker_project_code_search" model="ir.ui.view">
            <field name="name">project.project.code</field>
            <field name="model">project.project</field>
            <field name="inherit_id" ref="project.edit_project"/>
            <field name="arch" type="xml">
              <xpath expr="//sheet/notebook/page/group/group/field[@name='company_id']" position="after">
                    <field name='rate_per_shift'/>
                  <field name="no_of_shift"/>
              </xpath>


            </field>
    </record>


         <record id="caretaker_work_entry_type_form" model="ir.ui.view">
        <field name="name">form</field>
        <field name="model">hr.work.entry.type</field>
        <field name="inherit_id" ref="hr_work_entry.hr_work_entry_type_view_form"/>
        <field name="arch" type="xml">

               <xpath expr="//sheet/group/group/field[@name='sequence']" position="after">

                   <field name="no_of_shift"/>

            </xpath>
        </field>
     </record>

    </data>
</odoo>