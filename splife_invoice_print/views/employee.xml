<odoo>
    <data>
        <record id="hr_employee_payslip_form" model="ir.ui.view">
        <field name="name">hr.employee.invoice print</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr.view_employee_form"/>
        <field name="arch" type="xml">


             <xpath expr="//sheet/notebook/page[@name='personal_information']/group/group/field[@name='passport_id']" position="after">

                <field name="acc_number"/>
            </xpath>

        </field>
        </record>
    </data>
</odoo>