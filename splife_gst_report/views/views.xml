<odoo>
    <data>
        <record id="daily_gst_report_wizard_view" model="ir.ui.view">
            <field name="name">gst.report.wizard Report</field>
            <field name="model">gst.report.wizard</field>
            <field name="arch" type="xml">


                <form>
                    <field invisible="1" name="state"/>
                    <div states="choose">
                        <group col="4" >
                            <field name="date_from" required="1"/>
                            <field name="date_to" required="1"/>

                        </group>

                         <group>
                             <field name="company_id" required="1"/>
                     </group>
                    </div>
                    <div states="get">
                        <group>
                            <field name="name" colspan="4" invisible="1"/>
                            <field name="report" filename="name" colspan="4"/>
                        </group>
                    </div>
                    <footer states="choose">
                        <button name="generate_gstrsale_report" string="Generate GST Report" type="object"
                                class="oe_highlight"/>
                        or
                        <button special="cancel" string="Cancel" class="oe_highlight"/>
                    </footer>
                    <footer states="get">
                        <button special="cancel" string="Cancel" class="oe_highlight"/>
                    </footer>
                </form>
<!--                <form string="Report Options">-->
