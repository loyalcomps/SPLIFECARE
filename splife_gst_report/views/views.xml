<!--                                string="Excel Print" class="oe_highlight"/>-->
<!--                        or-->

<!--                        <button string="Cancel" class="oe_link" special="cancel"/>-->
<!--                    </footer>-->
<!--                </form>-->
            </field>
        </record>


        <record id="daily_gst_report_wizard" model="ir.actions.act_window">
            <field name="name">GST Report</field>
            <field name="res_model">gst.report.wizard</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="daily_gst_report_wizard_view"/>
            <field name="target">new</field>
        </record>


      <menuitem id="menu_daily_gst" name="GST Report"
                  parent="account.menu_finance_reports" action="daily_gst_report_wizard"
                  sequence="6"/>


