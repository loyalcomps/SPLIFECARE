<odoo>
    <data>
         <record id="splife_gst_report_form_asset_inherit" model="ir.ui.view">
                <field name="name">account.account.form</field>
                <field name="model">account.account</field>
                <field name="inherit_id" ref="account.view_account_form"/>
                <field name="arch" type="xml">
                    <field name="user_type_id" position="after">
                        <field name="gst_report"/>
                        <field name="gst_report_accounts"/>
                    </field>
                </field>
         </record>


         <record id="splife_gst_account_tax_page" model="ir.ui.view">
                <field name="name">account.move.form</field>
                <field name="model">account.move</field>
                 <field name="inherit_id" ref="account.view_move_form"/>
                <field name="arch" type="xml">
                    <xpath expr="//sheet/notebook/page[@name='other_info']" position="after">
                         <page string="Tax">
                             <field name="account_gst_tax_ids">
                                <tree editable="bottom" string="Tax">

                                         <field name="move_id"/>
                                        <field name="karkkidaka_kit_account"/>
                                        <field name="cgst_tax"/>
                                        <field name="sgst_tax"/>
                                         <field name="kfc_tax"/>
                                        <field name="registration_fee_account"/>
                                    <field name="reg_cgst_tax"/>
                                    <field name="reg_sgst_tax"/>
                                        <field name="reg_kfc_tax"/>
                                </tree>
                            </field>

                        </page>
                    </xpath>
                </field>
         </record>




    </data>
</odoo>